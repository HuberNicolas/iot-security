      <breakpoints />
      <interface_config>
        se.sics.cooja.interfaces.Position
        <x>90.2206937250823</x>
        <y>30.89269963809436</y>
        <z>0.0</z>
      </interface_config>
      <interface_config>
        se.sics.cooja.mspmote.interfaces.MspMoteID
        <id>1</id>
      </interface_config>
