      <success_ratio_rx>1.0</success_ratio_rx>
    </radiomedium>
    <events>
      <logoutput>40000</logoutput>
    </events>
    <motetype>
      se.sics.cooja.mspmote.SkyMoteType
      <identifier>sky1</identifier>
      <description>receiver</description>
      <source EXPORT="discard">[CONTIKI_DIR]/examples/ipv6/rpl-udp/receiver_authentication-w3-ex1.c</source>
      <commands EXPORT="discard">make receiver_authentication-w3-ex1.sky TARGET=sky</commands>
      <firmware EXPORT="copy">[CONTIKI_DIR]/examples/ipv6/rpl-udp/receiver_authentication-w3-ex1.sky</firmware>
